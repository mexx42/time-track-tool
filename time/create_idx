create index _daily_record_date_idx on _daily_record ( _date ) ;
create index _daily_record_user_idx on _daily_record ( _user ) ;
create index _time_record_wp_idx on _time_record ( _wp ) ;
create index _time_record_daily_record_idx on _time_record ( _daily_record ) ;
create index _time_record_wp_daily_record_idx on _time_record ( _wp, _daily_record ) ;
create index _time_record_time_activity_idx on _time_record (_time_activity) ;
create index _time_record_time_activity_daily_record_idx on _time_record (_time_activity, _daily_record) ;

create index _it_issue_status_idx on _it_issue (_status) ;
create index _it_issue_it_prio_idx on _it_issue (_it_prio) ;
create index _it_issue_category_idx on _it_issue (_category) ;
create index _it_issue_it_project_idx on _it_issue (_it_project) ;
create index _it_issue_responsible_idx on _it_issue (_responsible) ;
create index _it_issue_stakeholder_idx on _it_issue (_stakeholder) ;
